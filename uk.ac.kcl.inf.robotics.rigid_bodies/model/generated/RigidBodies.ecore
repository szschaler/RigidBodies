<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="rigidBodies" nsURI="http://www.ac.uk/kcl/inf/robotics/RigidBodies"
    nsPrefix="rigidBodies">
  <eClassifiers xsi:type="ecore:EClass" name="Model">
    <eStructuralFeatures xsi:type="ecore:EReference" name="defs" upperBound="-1" eType="#//InitialDefinition"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="world" eType="#//Environment"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="bodies" upperBound="-1"
        eType="#//System" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InitialDefinition">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Environment">
    <eStructuralFeatures xsi:type="ecore:EReference" name="gravity" eType="#//Vector3"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="System">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="elements" upperBound="-1"
        eType="#//SystemElement" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SystemElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="Body" eSuperTypes="#//SystemElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="frame" eType="#//LocalFrame"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mass" eType="#//Mass" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="LocalFrame">
    <eStructuralFeatures xsi:type="ecore:EReference" name="orientation" eType="#//Matrix3X3"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="position" eType="#//Vector3"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Mass">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="position" eType="#//Vector3"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="inertia" eType="#//Matrix3X3"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Joint" eSuperTypes="#//SystemElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="#//Matrix4X4"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="body1" eType="#//Body"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="relTransformation1" eType="#//Matrix4X4"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="body2" eType="#//Body"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="relTransformation2" eType="#//Matrix4X4"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Constraint" eSuperTypes="#//SystemElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="ColocationConstraint" eSuperTypes="#//Constraint">
    <eStructuralFeatures xsi:type="ecore:EReference" name="joint1" eType="#//Joint"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="joint2" eType="#//Joint"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Vector3"/>
  <eClassifiers xsi:type="ecore:EClass" name="BaseVector3" eSuperTypes="#//InitialDefinition #//Vector3"/>
  <eClassifiers xsi:type="ecore:EClass" name="Vector3Ref" eSuperTypes="#//Vector3">
    <eStructuralFeatures xsi:type="ecore:EReference" name="vector" eType="#//BaseVector3"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Matrix3X3"/>
  <eClassifiers xsi:type="ecore:EClass" name="BaseMatrix3X3" eSuperTypes="#//Matrix3X3 #//MatrixDef"/>
  <eClassifiers xsi:type="ecore:EClass" name="Matrix3X3Ref" eSuperTypes="#//Matrix3X3">
    <eStructuralFeatures xsi:type="ecore:EReference" name="matrix" eType="#//BaseMatrix3X3"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Matrix4X4"/>
  <eClassifiers xsi:type="ecore:EClass" name="BaseMatrix4X4" eSuperTypes="#//Matrix4X4 #//MatrixDef"/>
  <eClassifiers xsi:type="ecore:EClass" name="Matrix4X4Ref" eSuperTypes="#//Matrix4X4">
    <eStructuralFeatures xsi:type="ecore:EReference" name="matrix" eType="#//BaseMatrix4X4"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MatrixDef" eSuperTypes="#//InitialDefinition"/>
</ecore:EPackage>
